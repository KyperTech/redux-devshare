{
  "name": "redux-devshare",
  "version": "0.1.4",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "files": [
    "src",
    "dist",
    "lib"
  ],
  "scripts": {
    "clean": "rimraf lib dist coverage",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "mocha -R spec --compilers js:babel-core/register ./test/setup.js ./test/**/*.spec.js",
    "test:cov": "babel-node $(npm bin)/isparta cover --report lcov ./node_modules/mocha/bin/_mocha -- ./test --recursive",
    "build:lib": "babel src --out-dir lib",
    "build": "npm run clean && npm run build:lib",
    "watch:lib": "npm run build:lib -- --stats --progress --colors --watch",
    "watch": "npm run watch:lib",
    "prepublish": "npm run clean && npm run lint:fix && npm run test && npm run build",
    "prepush": "npm run lint:fix",
    "upload": "node ./bin/upload"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kypertech/redux-devshare.git"
  },
  "keywords": [
    "kyper",
    "devshare",
    "redux",
    "tessellate",
    "authentication"
  ],
  "author": "kypertech",
  "contributors": [
    {
      "name": "prescott prue",
      "email": "sprue.dev@gmail.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kypertech/redux-devshare/issues"
  },
  "homepage": "https://github.com/kypertech/redux-devshare",
  "dependencies": {
    "devshare": "^0.1.4",
    "humps": "1.0.0",
    "lodash": "^4.14.2",
    "normalizr": "2.2.1",
    "updeep": "^0.16.1"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-lodash": "^3.2.6",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "eslint": "^3.2.2",
    "eslint-config-standard": "^5.3.5",
    "eslint-config-standard-react": "^2.5.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "eslint-plugin-standard": "^2.0.0",
    "husky": "^0.11.6",
    "isparta": "^4.0.0",
    "json-loader": "^0.5.4",
    "mocha": "^3.0.2",
    "rimraf": "^2.5.4",
    "s3-cli": "^0.13.0",
    "webpack": "^1.13.1"
  }
}
