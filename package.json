{
  "name": "redux-devshare",
  "version": "0.0.5",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "files": [
    "src",
    "dist",
    "lib"
  ],
  "scripts": {
    "clean": "rimraf lib dist coverage",
    "lint": "standard ./src/**",
    "test": "mocha -R spec --compilers js:babel-core/register ./test/setup.js ./test/**/*.spec.js",
    "test:cov": "babel-node $(npm bin)/isparta cover --report lcov ./node_modules/mocha/bin/_mocha -- ./test --recursive",
    "build:lib": "babel src --out-dir lib",
    "build": "npm run clean && npm run build:lib",
    "watch:lib": "npm run build:lib -- --stats --progress --colors --watch",
    "watch": "npm run watch:lib",
    "prepublish": "npm run clean && npm run test && npm run build",
    "upload": "node ./bin/upload"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kypertech/redux-devshare.git"
  },
  "keywords": [
    "kyper",
    "matter",
    "tessellate",
    "authentication",
    "user",
    "project management"
  ],
  "author": "kypertech",
  "contributors": [
    {
      "name": "kypertech",
      "email": "kyper.dev@gmail.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kypertech/redux-devshare/issues"
  },
  "homepage": "https://github.com/kypertech/redux-devshare",
  "dependencies": {
    "devshare": "^0.0.5",
    "humps": "1.0.0",
    "lodash": "^4.6.1",
    "normalizr": "2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-lodash": "^2.2.1",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "isparta": "^4.0.0",
    "json-loader": "^0.5.4",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.1",
    "s3-cli": "^0.13.0",
    "standard": "^6.0.8",
    "webpack": "^1.12.14"
  }
}
