{
  "name": "redux-devshare",
  "version": "0.2.0",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "files": [
    "src",
    "dist",
    "lib"
  ],
  "scripts": {
    "clean": "rimraf lib dist coverage",
    "lint": "eslint ./src",
    "lint:fix": "npm run lint -- --fix",
    "test": "mocha -R spec --compilers js:babel-core/register ./test/setup.js ./test/**/*.spec.js",
    "test:cov": "babel-node $(npm bin)/isparta cover --report lcov ./node_modules/mocha/bin/_mocha -- ./test --recursive",
    "build:lib": "babel src --out-dir lib",
    "build": "npm run clean && npm run build:lib",
    "watch:lib": "npm run build:lib -- --stats --progress --colors --watch",
    "watch": "npm run watch:lib",
    "prepublish": "npm run clean && npm run lint:fix && npm run test && npm run build",
    "prepush": "npm run lint:fix",
    "upload": "node ./bin/upload"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kypertech/redux-devshare.git"
  },
  "keywords": [
    "kyper",
    "devshare",
    "redux",
    "authentication"
  ],
  "author": "kypertech",
  "contributors": [
    {
      "name": "prescott prue",
      "email": "sprue.dev@gmail.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kypertech/redux-devshare/issues"
  },
  "homepage": "https://github.com/kypertech/redux-devshare",
  "dependencies": {
    "devshare": "^0.2.0",
    "es6-promise": "^3.2.1",
    "firebase": "^3.3.0",
    "immutable": "^3.7.6",
    "lodash": "*"
  },
  "peerDependencies": {
    "react": "^0.14.6 || ^15.0.0",
    "react-redux": "^4.0.6",
    "redux": "^3.0.5"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-lodash": "^3.2.9",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "chai": "^3.5.0",
    "eslint": "^3.6.1",
    "eslint-config-standard": "^6.1.0",
    "eslint-config-standard-react": "^4.1.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^6.3.0",
    "eslint-plugin-standard": "^2.0.1",
    "husky": "^0.11.8",
    "isparta": "^4.0.0",
    "json-loader": "^0.5.4",
    "mocha": "^3.1.0",
    "rimraf": "^2.5.4",
    "s3-cli": "^0.13.0",
    "webpack": "^1.13.2"
  }
}
